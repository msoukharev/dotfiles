alias gita="git add ."

gitc() {
    git commit -m $@
}

# git add . ; git commit -m $1
gitac() {
    git add . ; git commit -m $@
}

# git push origin $1
gitps() {
    git push origin $@
}

# git pull origin $1
gitpl() {
    git pull origin $@
}

gitpushso() {
    git push --set-upstream origin $@
}